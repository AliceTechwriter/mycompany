MODULE BillItem;

REQUIRE Bill;

NAMESPACE Invoicing;

EXTEND FORM item
    OBJECTS bl = BillLine
    PROPERTIES(bl) READONLY nameStatus, number, dateTime, nameVendor, nameCompany, quantity, price
    PROPERTIES(bl) NEWSESSION EDIT
    FILTERS item(bl) = i
;

DESIGN item {
    operations {
        MOVE BOX(bl) { caption = 'Bills';  }
    }
}