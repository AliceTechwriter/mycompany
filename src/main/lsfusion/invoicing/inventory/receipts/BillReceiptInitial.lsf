MODULE BillReceiptInitial;

REQUIRE BillReceipt;

NAMESPACE Invoicing;

// initial data
loadDefaultData () + {
    receiptType(BillType t) <- receiptType('receipt') WHERE id(t) = 'bill'; 
    receiptType(BillType t) <- receiptType('return') WHERE id(t) = 'creditnote' OR id(t) = 'retailreturn';
    autoCreateReceipt(BillType t) <- TRUE WHERE id(t) = 'creditnote' OR id(t) = 'retailreturn';
}